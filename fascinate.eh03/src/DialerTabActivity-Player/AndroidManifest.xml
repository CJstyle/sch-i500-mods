<?xml version="1.0" encoding="utf-8"?>
<manifest android:sharedUserId="android.uid.shared" android:versionCode="10" android:versionName="2.3.5" package="com.sec.android.app.dialertab"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="com.samsung.vvm.provider.Permission.ACCESS_VVMPROVIDER" />
    <uses-permission android:name="android.permission.CALL_PRIVILEGED" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.MODIFY_PHONE_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.PERSISTENT_ACTIVITY" />
    <uses-permission android:name="android.permission.RAISED_THREAD_PRIORITY" />
    <application android:label="@string/launcherDialer" android:icon="@drawable/ic_launcher_phone" android:persistent="true" android:process="android.process.acore">
        <receiver android:label="DialerTabBootReceiver" android:name=".BootCompletedReceiver" android:enabled="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>
        <activity android:name=".dialer.DialerActivity" android:launchMode="singleTop" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.android.phone.action.TOUCH_DIALER" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
        </activity>
        <activity android:label="Call Log" android:name=".calllog.LogsListActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.sec.android.app.dialertab.calllog.LogsListActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:label="Call Log" android:name=".calllog.DetailViewActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.sec.android.app.dialertab.calllog.DetailViewActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.sec.android.app.dialertab.calllog.CallDurationActivity" android:screenOrientation="portrait" />
        <activity android:name="com.sec.android.app.dialertab.calllog.LogDeleteActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.sec.android.app.dialertab.calllog.LogDeleteActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.sec.android.app.dialertab.calllog.LogDetailDeleteActivity" android:screenOrientation="portrait" />
        <activity android:name="com.sec.android.app.dialertab.frequent.FrequentDeleteActivity" android:screenOrientation="portrait" />
        <activity android:name="com.sec.android.app.dialertab.frequent.FrequentGridDeleteActivity" android:screenOrientation="portrait" />
        <activity android:name="com.sec.android.app.dialertab.frequent.FrequentListActivity" android:screenOrientation="portrait" />
        <activity android:theme="@style/DialtactsTheme" android:label="@string/launcherDialer" android:name=".DialerTabActivity" android:clearTaskOnLaunch="true" android:launchMode="singleTask" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:mimeType="vnd.android.cursor.item/phone" />
                <data android:mimeType="vnd.android.cursor.item/person" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="voicemail" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.DIAL" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="tel" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:mimeType="vnd.android.cursor.dir/calls" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CALL_BUTTON" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
        <uses-library android:name="touchwiz" />
        <uses-library android:name="sec_feature" />
        <activity android:theme="@android:style/Theme.NoDisplay" android:name="TransDialerTab" />
    </application>
    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="10" />
</manifest>
